<html lang="ja">
<body>
<script type="text/javascript">

document.write("Mia's CracklePop","<br>");

sum = 0;
for (i = 1; i < 101; i = i ++)
{
if (i % 3 == 0 && i % 5 == 0){document.write("CrackPop","<br>");}
else if (i % 3 == 0){document.write("Crackle","<br>");}
else if (i % 5 == 0){document.write("Pop","<br>");}
else{document.write(i,"<br>");}}

</script>
</body>
</html>
